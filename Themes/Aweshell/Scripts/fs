DNG="<table valign=\"middle\" cellspacing=\"0\" cellpadding=\"0\" font-family: angkor; font-size: 9pt;><tr><td style=\"color: rgba(244,244,244,100%)\">$1.</td><td style=\"color: rgba(255,0,0,100%)\">$SPC%</td></tr></table>"
NOR="<table valign=\"middle\" cellspacing=\"0\" cellpadding=\"0\" font-family: angkor; font-size: 9pt; color: rgba(244,244,244,100%)><tr><td>$1.</td><td>$SPC%</td></tr></table>"

if [[ $SPC > 90 ]]; then
  echo "$DNG"
elsif
  echo "$NOR"
else
  echo "No volume found"
fi 
