[BE::Desk]
Corners=80
IconPos=484ee7:110:60,10168c1:110:157,
IconSize=64
ShowIcons=false
Tint=invalid
TrashSize=64
TrashX=26
TrashY=51
Wallpaper=/home/nemo/immagini/sfondi/bot/wallhaven-45380.jpg
WallpaperAlign=132
WallpaperAspect=-1
WallpaperMode=7

[BE::Shell]
Panels=TopPanel,BottomPanel,Dockbar
Theme=Aweshell

[Be::Desk]
BlurRadius=0
HaloColor=invalid
Rottpaper=true
ShadowOpacity=80
ShowIcons=false
TrashCan=false

[BottomPanel]
Applets=mpd,stretch,clock
BlurRadius=0
CastShadow=true
Frozen=false
Id=BottomPanel
Layer=0
Length=100
Offset=0
Position=1
Screen=-1
Size=22
Struts=true
Visible=true

[TopPanel]
Applets=start,pager,stretch,infocenter,up
BlurRadius=0
CastShadow=true
Frozen=false
Id=TopPanel
Layer=0
Length=100
Offest=0
Position=0
Screen=-1
Size=22
Struts=true
Visible=true

[Dockbar]
Applets=dock,stretch
BlurRadius=0
CastShadow=false
Frozen=false
Id=Dockbar
Layer=0
Length=94
Offset=3
Position=2
Screen=-1
Size=24
Struts=true
Visible=false

[dock]
Applets=task
Position=2
BlurRadius=100
Frozen=true
Id=dock
Type=Panel

[task]
AlwaysGroup=True
ButtonMode=0
Buttons=file-manager,browser,konsole,editor,irc,gwenview,ksysguard,settings,darkroom
IconSize=20
OnlyCurrentDesk=False
OnlyMinimized=False
Spacing=0
Type=Taskbar

[file-manager]
Exec=dfm
Icon=dolphin
IconSize=20
Label=File Manager
Mode=0
Type=Button

[editor]
Exec=kate
Icon=kate
IconSize=20
Label=Editor
Mode=0
Type=Button

[irc]
Exec=konversation
Icon=konversation
IconSize=20
Label=IRC
Mode=0
Type=Button

[browser]
Exec=firefox
Icon=firefox
IconSize=20
Label=Browser
Mode=0
Type=Button

[darkroom]
Exec=darktable
Icon=darktable
IconSize=20
Label=Darkroom
Mode=0
Type=Button

[clock]
Pattern='<html>'ddd. dd MMM. hh:mm ap'</html>'
Type=Clock

[mpd]
Exec=$HOME/.local/share/be.shell/Scripts/applet mpc mpc
Lines=1
PermittedCommands=mpc toggle
PollInterval=1000
Type=Label

[up]
Exec=$HOME/.local/share/be.shell/Scripts/applet up
Lines=1
PollInterval=600000
Type=Label

[start]
DBus=session;org.kde.be.shell;/Shell;org.kde.be.shell;togglePanel;Dockbar
Label=Start
Icon=start
IconSize=22
Mode=0
Type=Button

[volume]
Channel=Master
MixerCommand=kmix
Step=5%
Mode=1
Type=Volume

[battery]
Exec=kcmshell5 powerdevilglobalconfig
FillIsCharge=True
Type=Battery
WheelDown=qdbus org.kde.kglobalaccel /component/kded5 invokeShortcut "Increase Screen Brightness"
WheelUp=qdbus org.kde.kglobalaccel /component/kded5 invokeShortcut "Decrease Screen Brightness"
